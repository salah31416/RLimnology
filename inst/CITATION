
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "manual",
         title = "eeiR: Ecological Evaluation Index in R",
         author = personList(as.person("Izi Sousa-Filho"),
           as.person("Lorena Pinheiro-Silva"),
           as.person("Vanessa Lopes" )),
         year = year,
         note = note,
		 url = "https://github.com/salah31416/RLimnology")
